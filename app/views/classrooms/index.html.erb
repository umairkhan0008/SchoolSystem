<table class="table">
  <thead>
    <tr>
      <th scope="col">Section</th>
      <th scope="col">course</th>
      <th scope="col">Admin name</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @classrooms.each do |kk|%>
      <tr>
        <td><%=  kk.section %></td>
        <td><%=  kk.course %></td>
        <td><%= kk.user.name%></td>
        <td><%=  link_to "Edit",  edit_classroom_path(kk.id)%></td>
        <td><%=  link_to "Show", classroom_path(kk.id)%></td>
        <td><%=  button_to  'Delete', classroom_path(kk), method: 'delete' %></td>
    <% end%>
  </tbody>
</table>
