<h1>Welcome To Student Data</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Father</th>
      <th scope="col">Classroom_id</th>
      <th scope="col">Action</th>

    </tr>
  </thead>
  <tbody>
    <% @students.each do |kk|%>
      <tr>
        <td><%=  kk.name %></td>
        <td><%=  kk.father_name %></td>
        <td><%= kk.classroom.section%></td>
        <td><%=  link_to "Edit",  edit_student_path(kk.id)%></td>
        <td><%=  link_to "Show", student_path(kk.id)%></td>
        <td><%=  button_to  'Delete', student_path(kk), method: 'delete' %></td>
    <% end%>
  </tbody>
</table>